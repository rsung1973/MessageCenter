@using System.IO
@using System.Linq.Expressions
@using Utility
@using Newtonsoft.Json
@using Newtonsoft.Json.Linq
@{
    IQueryable<dynamic> _model;
    _model = (IQueryable<dynamic>)this.Model;
}
<div class="border_gray" style="overflow-x: auto; max-width: 1024px;">
    @{
        var recordCount = _model.Count();
    }
    @if (recordCount > 0)
    {
        Html.RenderPartial("~/Views/DataQuery/ItemList.cshtml", _model);
    }
    else
    {
        <font color="red">查無資料!!</font>
    }
</div>


